{
  "name": "koutu",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "shared",
    "frontend",
    "backend"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "npm run start -w backend",
    "start:frontend": "npm run start -w frontend",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "npm run dev -w backend",
    "dev:frontend": "npm run dev -w frontend",
    "test": "npm run test --workspaces",
    "test:backend": "npm run test -w backend",
    "test:frontend": "npm run test -w frontend",
    "test:shared": "npm run test -w shared",
    "test:watch": "npm run test:watch --workspaces",
    "test:coverage": "npm run test:coverage --workspaces",
    "pretest": "npm run build -w shared",
    "test:path": "node scripts/run-tests.js",
    "postinstall": "chmod +x scripts/run-tests.js",
    "test:backend-only": "cd backend && npx jest --config jest.config.cjs --runInBand",
    "test:frontend-only": "cd frontend && npx jest --config jest.config.cjs",
    "test:shared-only": "cd shared && npx jest --config jest.config.cjs"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/archiver": "^6.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.11.18",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-query": "^1.2.8",
    "@types/supertest": "^6.0.3",
    "concurrently": "^8.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.6.1",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.75.2",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.16.0",
    "zod": "^3.24.3"
  }
}
