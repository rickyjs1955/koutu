{
  "name": "@koutu/frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "cross-env VITE_CJS_IGNORE_WARNING=true vitest",
    "test:run": "cross-env VITE_CJS_IGNORE_WARNING=true vitest run",
    "test:ui": "cross-env VITE_CJS_IGNORE_WARNING=true vitest --ui",
    "test:coverage": "cross-env VITE_CJS_IGNORE_WARNING=true vitest run --coverage",
    "test:watch": "cross-env VITE_CJS_IGNORE_WARNING=true vitest watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.1",
    "@koutu/shared": "*",
    "@tanstack/react-query": "^4.36.1",
    "axios": "^1.4.0",
    "framer-motion": "^12.23.6",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.1",
    "react-konva": "^18.2.10",
    "react-router-dom": "^6.14.1",
    "vitest": "^3.2.4",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.7.0",
    "eslint": "^8.44.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6",
    "vite": "^7.0.0"
  }
